{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "no-alert": 0,
    "no-bitwise": 0,
    "camelcase": 1,
    "curly": 1,
    "eqeqeq": 1,
    "no-console": 1,
    "no-eq-null": 0,
    "guard-for-in": 1,
    "no-empty": 1,
    "no-unused-vars": 1,
    "no-mixed-spaces-and-tabs": 1,
    "quotes": [ 1, "single" ]
  },
  "env": {
    "browser": true
  }
}
